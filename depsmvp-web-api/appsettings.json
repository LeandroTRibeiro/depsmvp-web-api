{
  "ConnectionStrings": {
    "DefaultConnection": "${DB_CONNECTION_STRING}"
  },
  "ExternalServices": {
    "BrasilApi": {
      "Url": "${BRASIL_API_URL}"
    },
    "PortalDaTrasparenciaApi": {
      "BaseUrl": "${PORTAL_TRANSPARENCIA_BASE_URL}",
      "Header": {
        "Authorization": {
          "Name": "chave-api-dados",
          "Value": "${PORTAL_TRANSPARENCIA_API_KEY}"
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
